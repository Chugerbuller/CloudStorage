<UserControl
    x:Class="CloudStore.UI.UCListItem"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">


    <Grid Name="MainGrid" ColumnDefinitions="*,4*,*">
        <Image Grid.Column="0" x:Name="ImageExt" />
        <TextBox Grid.Column="1" x:Name="FileNameBox" />
        <StackPanel
            x:Name="ButtonPanel"
            Grid.Column="2"
            Orientation="Horizontal" />
    </Grid>


</UserControl>
